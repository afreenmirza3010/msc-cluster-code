{
    "dataset_params": {
        "class_name": "HDF5Reader",
        "config": {
            "filename": "../script/head_neck_data.h5",
            "batch_size":1 ,
            "x_name": "input",
            "y_name": "target",
            "batch_cache": 1
        }
        }
    ,
    "train_params": {
        "epochs": 2,
        "callbacks": [

            {
                "class_name": "ModelCheckpoint",
                "config": {
                    "monitor": "val_loss",
                    "filepath": "../../head_neck_data.{epoch:02d}-{val_loss:.2f}.h5",
                    "period": 1
                }
            }
        ]
    },
    "input_params": {
        "shape": [173,
            191,
            265,
            2
        ]
    },
    "model_params": {
        "loss": "categorical_crossentropy",
        "optimizer": {
            "class_name": "adam"
        },
        "metrics": [
            "accuracy"
        ]
    },
    "loss": {
        "class_name": "BinaryFbetaLoss"
    },
    "optimizer": {
        "class_name": "adam",
        "config": {
            "learning_rate": 0.0001
        }
    },
    "metrics": [
        {
            "class_name": "BinaryFbeta"
        },
        "TruePositives",
        "FalsePositives",
        "FalseNegatives"
    ],
    "architecture": {
        "type": "Vnet",
        "layers": [
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 4,
                    "kernel_size": 3,
                    "activation": "relu",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "name": "conv2",
                "class_name": "Conv3D",
                "config": {
                    "filters": 4,
                    "kernel_size": 3,
                    "activation": "relu",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "MaxPooling3D"
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 8,
                    "kernel_size": 3,
                    "activation": "relu",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "name": "conv_T_4",
                "class_name": "Conv3DTranspose",
                "config": {
                    "filters": 64,
                    "kernel_size": 3,
                    "strides": 1,
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 4,
                    "kernel_size": 3,
                    "activation": "relu",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                },
                "inputs": [
                    "conv2",
                    "conv_T_4"
                ]
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 4,
                    "kernel_size": 3,
                    "activation": "relu",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                },
                "normalizer": {
                    "class_name": "BatchNormalization"
                }
            },
            {
                "class_name": "Conv3D",
                "config": {
                    "filters": 1,
                    "kernel_size": 3,
                    "activation": "sigmoid",
                    "kernel_initializer": "he_normal",
                    "padding": "same"
                }
            }
        ]
    }
}